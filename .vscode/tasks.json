{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build",
			"type": "npm",
			"script": "build",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": [
				"$tslint5",
				"$tsc"
			]
		},
		{
			"label": "test",
			"type": "npm",
			"script": "test",
			"group": {
				"kind": "test",
				"isDefault": true
			},
			"problemMatcher": [
				"$tslint5",
				"$tsc"
			]
		},
		{
			"type": "npm",
			"script": "lint",
			"problemMatcher": [
				"$tslint5"
			]
		}
	]
}
